{
  "name": "nextjs-openai",
  "version": "3.0.1-canary.0",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "scripts": {
    "export": "tsmodule build",
    "docs": "yarn typedoc src",
    "dev": "next dev",
    "prebuild": "yarn docs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "pretest": "tsmodule build",
    "test": "ava",
    "prepublishOnly": "yarn export && yarn test",
    "canary": "yarn build && yarn version --prerelease --preid=canary && npm publish --tag canary"
  },
  "devDependencies": {
    "@tsmodule/react": "^12.0.0",
    "@tsmodule/tsmodule": "^42.1.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.4.13",
    "ava": "^5.2.0",
    "cssnano": "^5.1.15",
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.1.6",
    "next": "^13.1.6",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.2.7",
    "typedoc": "^0.23.25",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "eventsource-parser": "^0.1.0",
    "openai-streams": "^1.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yield-stream": "^2.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "nextjs",
    "openai",
    "react",
    "typescript",
    "streams",
    "gpt-3"
  ],
  "description": "Hooks and components for working with OpenAI streams.",
  "homepage": "https://github.com/gptlabs/nextjs-openai"
}
